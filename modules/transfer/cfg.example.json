{
    "debug": true,
    "minStep": 30,
    "http": {
        "enabled": true,
        "listen": "0.0.0.0:6060"
    },
    "rpc": {
        "enabled": true,
        "listen": "0.0.0.0:8433"
    },
    "socket": {
        "enabled": true,
        "listen": "0.0.0.0:4444",
        "timeout": 3600
    },
    "judge": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            {% for host in groups['judge']%}
            "judge-{{ hostvars[host]['ansible_nodename']}}":"{{ hostvars[host]['ansible_ssh_host'] }}:6080"{% if not loop.last %},{% endif %}
            {% endfor %}
        }
    },
    "graph": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            {% for host in groups['graph']%}
            "graph-{{ hostvars[host]['ansible_nodename']}}":"{{ hostvars[host]['ansible_ssh_host'] }}:6070"{% if not loop.last %},{% endif %}
            {% endfor %}
        }
    },
    "tsdb": {
        "enabled":true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": {% for host in groups['tsdb']%} "{{ hostvars[host]['ansible_ssh_host'] }}:4242"{% if not loop.last %},{% endif %} {% endfor %}
    }
}
